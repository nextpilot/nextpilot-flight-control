Import('rtconfig')
from building import *

src	= []

if rtconfig.PLATFORM in ['armcc', 'armclang']: # Keil-MDK
    src += Glob('*_rvds.S')

if rtconfig.PLATFORM == 'gcc':
    src += Glob('*_gcc.S')

if rtconfig.PLATFORM == 'iccarm': # IAR
    src += Glob('*_iar.S')

group = DefineGroup('rtt/Kernel', src, depend = ['PKG_USING_RT_MEMCPY_CM'])

Return('group')
